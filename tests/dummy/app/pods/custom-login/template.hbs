{{frost-login
  brandingStrip='app/company-strip'
  logo='app/company'
  onSubmit='submitLogin'
}}
  {{!-- {{#if section.form}}
  {{frost-bunsen-form
    model=loginFormModel
    onChange=(action 'loginFormValueChanged')
    onValidation=(action 'onValidationHandler')
    value=loginFormValue
  }}
  {{/if}}
  {{#if section.actions}}
    {{frost-button
      disabled=(not valid)
      onClick=(action 'submitLogin')
      priority='primary'
      size='medium'
      text='Login'
    }}
  {{/if}} --}}
{{!-- {{/frost-login}} --}}
